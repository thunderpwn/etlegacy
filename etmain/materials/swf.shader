textures/swf/fuse_box_ad
{
    qer_editorimage textures/swf/fuse_box_ad.tga
	polygonOffset
	surfaceparm pointlight
	surfaceparm trans
	{
		diffusemap textures/swf/fuse_box_ad.tga
	    bumpmap textures/swf/fuse_box_ad_NORM.tga
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbGen vertex
		tcmod rotate 120
	}
}

